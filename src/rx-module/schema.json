{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsRxModule",
  "title": "Rx Module Schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Module name"
    },
    "container": {
      "type": "boolean",
      "description": "Is a container?"
    },
    "module": {
      "type": "string",
      "description": "Module name"
    },
    "dryRun": {
      "type": "boolean",
      "description": "Simulate?"
    }
  },
  "required": ["name"]
}
